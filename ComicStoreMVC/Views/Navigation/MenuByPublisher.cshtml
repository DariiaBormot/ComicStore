@using ComicStoreMVC.Models
@model IEnumerable<PublisherViewModel>

<ul>
    @{
        foreach (var item in Model)
        {
            <li>
                @Html.ActionLink(item.Name, "ComicBooksList", "ComicBooks", new { page = 1, publisherId = item.Id }, 
               new { @class = "btn btn-block btn-default btn-lg" })
            </li>
        }
    }
</ul>
