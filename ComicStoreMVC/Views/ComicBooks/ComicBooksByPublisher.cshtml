@*@model ComicStoreMVC.Models.ComicBookViewModel*@
@model PagedList.IPagedList<ComicStoreMVC.Models.ComicBookViewModel>
@using PagedList.Mvc;
<link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />


@foreach (var p in @Model)
{
    @Html.Partial("~/Views/Partials/ComicSummary.cshtml", p)
}


Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) from @Model.PageCount

@Html.PagedListPager(Model, page => Url.Action("ComicBooksByPublisher", new { page }))

